{
  "entrypoint": "index.html",
  "shell": "src/polymerday-app.html",
  "fragments": [
    "src/polymerday-home.html",
    "src/polymerday-cookies.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json",
    "firebase.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "name": "default",
    "preset": "es6-bundled",
    "basePath": false
  }]
}
